+	int err;
+
+	vmr = malloc(sizeof(*vmr));
+	if (!vmr)
+		return NULL;
+
+	err = ibv_cmd_reg_dmabuf_mr(pd, offset, length, iova, fd, access,
+#ifdef IBV_CMD_REG_DMABUF_MR_VER_2
+				    vmr, NULL);
+#else
+				    vmr);
+#endif
+	if (err) {
+		free(vmr);
+		errno = err;
+		return NULL;
+	}
+
+	return &vmr->ibv_mr;
+}
